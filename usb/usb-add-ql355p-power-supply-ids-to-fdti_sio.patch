+++ gregkh-2.6/drivers/usb/serial/ftdi_sio.h
@@ -339,6 +339,12 @@
 #define FTDI_SUUNTO_SPORTS_PID	0xF680	/* Suunto Sports instrument */
 
 /*
+ * TTi (Thurlby Thandar Instruments)
